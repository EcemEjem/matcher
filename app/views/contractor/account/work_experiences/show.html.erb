<div class="banner" style="background-image: linear-gradient(-225deg, rgba(0,101,168,0.6) 0%, rgba(0,36,61,0.6) 50%), url(<%= image_path 'nyc_try.png' %>);">
  <div class="banner-content">
    <h2>Work Experience</h2>
  </div>
</div>
<div class="wrapper-grey">
  <div class="container">
    <div class="row">

      <div class="col-md-8 col-md-offset-2 col-xs-12">
        <div class="box2">
            <table class="table">
              <tbody>
                <tr>
                  <td><em>Industry:</em></td>
                  <td><%= @work_experience.industry %></td>
                </tr>
                <tr>
                  <td><em>Experience:</em></td>
                  <td><%= @work_experience.experience %></td>
                </tr>
                <tr>
                <td><em>Company:</em></td>
                  <td><%= @work_experience.company %></td>
                </tr>
                <tr>
                  <td><em>Title:</em></td>
                  <td><%= @work_experience.title %></td>
                </tr>
                <tr>
                  <td><em>Start Date:</em></td>
                  <td><%= @work_experience.start_date %></td>
                </tr>
                <tr>
                  <td><em>End Date:</em></td>
                  <td><%= @work_experience.end_date %></td>
                </tr>
                <tr>
                        <tr>
                  <td><em>Duration:</em></td>
                  <td><%= @work_experience.duration %> years</td>
                </tr>
                <tr>
                  <td><em>Description:</em></td>
                  <td><%= @work_experience.description %></td>
                </tr>
                <tr>
                  <td>Edit your profile:</td>
                    <td><%= link_to contractor_account_work_experiences_path do %><i class="fa fa-arrow-left fa-1x" aria-hidden="true"></i>
                    <% end %>
                  <em><%= link_to 'Edit', edit_contractor_account_work_experience_path(@work_experience), class: "btn btn-secondary", id: "edit-button", style:"text-decoration:none" %></em>
                  </td>
                </tr>
              </tbody>
            </table>
        </div>     
      </div>

    </div>
  </div>
</div>


