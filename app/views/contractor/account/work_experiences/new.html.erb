<div class="wrapper-yellow"  style="background-color: #C63D0F ;">
    <h3 style="color:white;">
    Add your work experience and let the company to know about you</h3>
</div>
<div class="contenair" style="background-color:#FDF3E7;">
  <%= simple_form_for @work_experience, url: contractor_account_work_experiences_path, class:"panel panel-default" do |f| %>
    <%= f.input :industry %>
    <%= f.input :experience %>
    <%= f.input :company %>
    <%= f.input :title %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.input :description %>
    <%= f.submit %>
  <% end %>
</div>
